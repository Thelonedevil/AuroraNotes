<Window x:Class="AuroraNotes.NewPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AuroraNotes"
        mc:Ignorable="d"
        Title="NewStack" Height="300" Width="300">
	<Window.CommandBindings>
		<CommandBinding Command="Save" Executed="SaveCommand_Executed" CanExecute="SaveCommandBinding_CanExecute" />
	</Window.CommandBindings>
    <Grid>
		<Button x:Name="button" Command="Save" Content="Button" HorizontalAlignment="Left" Margin="207,239,0,0" VerticalAlignment="Top" Width="75"/>
		<TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="50,236,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

	</Grid>
</Window>
